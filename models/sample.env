# Copy this file to .env and fill in your API keys

# Required - OpenAI API Key
# Get it from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Required - Pinecone API Key  
# Get it from: https://www.pinecone.io/
PINECONE_API_KEY=your_pinecone_api_key_here

# Optional - Pinecone Index Name (default: course-recommendations)
PINECONE_INDEX_NAME=course-recommendations

# Optional - Pinecone Environment/Region (default: us-east-1)
PINECONE_ENVIRONMENT=us-east-1
